numbers = [2, -93, -2, 8, None, -44, -1, -85, -14, 90, -22, -90, -100, -8, 38, -92, -45, 67, 53, 25]

length = len(numbers)
numb = [x for x in numbers if x is not None]
sum_numb = sum(numb)

miss_number = sum_numb / length
result = [miss_number if x is None else x for x in numbers]

print("Измененный список:", result)
